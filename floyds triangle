public static void floyds_triangle(int n) {
        int counter = 1;
        for (int i = 1; i <= n; i++) { // Outer
            for (int j = 1; j <= i; j++) { // Inner numbers - how many times will counter be printed
                System.out.print(counter + " ");
                counter++;
            }
            System.out.println();
        }
    }
